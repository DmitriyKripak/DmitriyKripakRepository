@using WebApplication2.Models
@model Dictionary<_Readership, string>

@{
    Layout = null;
}

@if (Model.Count > 0 )
{

    foreach (_Readership value in @Model.Keys)
    {
        <div class="items">
            <img src="@Url.Content(@Model[value])" width="90" height="150" />
            <H3>Who took : </H3>
            <p>@value.Login</p>
            <h3>Contact : </h3>
            <p>@value.Email</p>
        </div>
    }
}
else
{
<p>No data to display</p>
}